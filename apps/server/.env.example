# Server
PORT=3001

# Redis
REDIS_URL=redis://localhost:6379

# CoinGecko (optional - for pro API)
COINGECKO_API_KEY=

# Cache TTLs (in seconds)
CACHE_TTL_PRICES=300
CACHE_TTL_CHART_1D=300
CACHE_TTL_CHART_1W=900
CACHE_TTL_CHART_1M=3600
CACHE_TTL_CHART_1Y=86400

# Rate limiting
COINGECKO_CALLS_PER_MINUTE=5

# MongoDB
MONGODB_URI=mongodb://localhost:27017/pouch

# Alchemy Webhooks
# Auth token from your Alchemy account (Dashboard > Settings > Auth Token)
ALCHEMY_AUTH_TOKEN=

# Webhook 1 (e.g., Ethereum Sepolia for testing)
ALCHEMY_WEBHOOK_1_ID=
ALCHEMY_WEBHOOK_1_SIGNING_KEY=
ALCHEMY_WEBHOOK_1_NETWORK=

# Webhook 2 (e.g., Polygon Amoy for testing)
ALCHEMY_WEBHOOK_2_ID=
ALCHEMY_WEBHOOK_2_SIGNING_KEY=
ALCHEMY_WEBHOOK_2_NETWORK=MATIC_AMOY

# Webhook 3 (e.g., Base Sepolia for testing)
ALCHEMY_WEBHOOK_3_ID=
ALCHEMY_WEBHOOK_3_SIGNING_KEY=
ALCHEMY_WEBHOOK_3_NETWORK=BASE_SEPOLIA

# Add more webhooks as needed (up to 10)
# ALCHEMY_WEBHOOK_4_ID=
# ALCHEMY_WEBHOOK_4_SIGNING_KEY=
# ALCHEMY_WEBHOOK_4_NETWORK=ETH_MAINNET

# Expo Push (optional - for enhanced rate limits)
EXPO_ACCESS_TOKEN=
